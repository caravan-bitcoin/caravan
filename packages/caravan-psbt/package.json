{
  "name": "@caravan/psbt",
  "version": "2.0.3",
  "description": "typescript library for working with PSBTs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "files": [
    "./dist/index.js",
    "./dist/index.mjs",
    "./dist/index.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "npm run build -- --watch",
    "test": "vitest run src",
    "test:coverage": "vitest run src --coverage",
    "test:watch": "vitest src watch",
    "test:ui": "vitest --watch --ui",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "ci": "npm run lint && npm run test",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/vitest run"
  },
  "keywords": [
    "bitcoin",
    "psbt"
  ],
  "dependencies": {
    "@caravan/bip32": "*",
    "@caravan/bitcoin": "*",
    "bignumber.js": "^8.1.1",
    "bip174": "^2.1.1",
    "bitcoinjs-lib-v6": "npm:bitcoinjs-lib@^6.1.5",
    "buffer": "^6.0.3",
    "bufio": "^1.2.1",
    "uint8array-tools": "^0.0.7"
  },
  "author": "unchained capital",
  "license": "ISC",
  "devDependencies": {
    "@caravan/build-plugins": "*",
    "@caravan/eslint-config": "*",
    "@caravan/multisig": "*",
    "@caravan/typescript-config": "*",
    "eslint-plugin-prettier": "^5.1.3",
    "lodash": "^4.17.21",
    "react-silence": "^1.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@caravan/bitcoin": "0.4.2"
  }
}
